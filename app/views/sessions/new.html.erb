<%= form_tag(sessions_path) do %>
    <%= label_tag :name, 'Your Name' %>
    <%= select_tag 'user_name', options_for_select(@users.collect{ |u| [u.name] }), include_blank: true %>
    <%= label_tag :password, 'Your Password' %>
    <%= password_field_tag :password %>
    <%= submit_tag 'Sign In' %>
<% end %>
    